<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= name %></title>
  <meta name="description" content="<%= process.env.DESCRIPTION %>">
  <meta property="og:title" content="<%= name %>">
  <meta property="og:description" content="<%= process.env.DESCRIPTION %>">
  <meta property="og:image" content="/upload/logo.png">
  <meta property="og:type" content="website">
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:title" content="<%= name %>">
  <meta name="twitter:description" content="Short description for Twitter share">
  <meta name="twitter:image" content="/upload/logo.png">
  <meta name="robots" content="index, follow">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <script src="https://cdn.tailwindcss.com"></script>
  <link rel="icon" href="/upload/logo.png">
  <link href="https://fonts.googleapis.com/css2?family=Figtree:wght@300..900&display=swap" rel="stylesheet">
  <script src="https://unpkg.com/lucide@latest"></script>
</head>
